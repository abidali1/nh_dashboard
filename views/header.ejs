          <h5 class="modal-title" style="display:none" id="nodesModelNewsId">Add Notes</h5>

          <h5 class="modal-title" style="display:none" id="modelNewsId"></h5>
          <h5 class="modal-title" style="display:none" id="modelFolderId"></h5>

          <button type="button" class="close" data-dismiss="modal" aria-label="Close">
            <span aria-hidden="true">&times;</span>
          </button>
        </div>
        <form id="formNotes">
          <div id="addToFolderModelFolderList" class="modal-body">
  
            <div class="card-body" style="display: block;">
              <!-- Conversations are loaded here -->
              <div id="notesBlock" style="height:500px" class="direct-chat-messages">
                <!-- Message. Default to the left -->
        
          
